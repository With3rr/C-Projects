#include <iostream>
using namespace std;
int main()
{
	int vector[] = { 3, -5, 7, 10, -4, 14, 5, 2, -13 };
	int n = sizeof(vector) / sizeof(vector[0]);
	int *pointer = vector;
	int min = *pointer;


	for (int i = 1; i < n; i++)
	{
		pointer++;
		if (*pointer < min)
		{
			min = *pointer;
		}

		

	}

	cout << min;

	return 0;

}
