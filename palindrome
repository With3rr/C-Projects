
#include <iostream>
using namespace std;

int main()
{
	int vector[] = {4,3, 1,2,1,3,4};
	bool ispalindrome = true;
	int n = sizeof(vector) / sizeof(vector[0])-1;
	
	for (int i = 0; i < (((sizeof(vector) / sizeof(vector[0]))/2)+1); i++)
	{

		if (vector[i] == vector[n])
		{


		}
		else
		{
			ispalindrome = false;
		}
		n--;
	}

	if (ispalindrome)
		cout << "It's a palindrome";
	else
		cout << "It isn't a palindrome";
	cout << endl;

    
	return 0;
}
