#include <iostream>
#include <cstdlib>
#include <string>

using namespace std;

int main()
{
	int vector1[7] = { 4, 7, 2, 8, 1, 3, 0 };
	int vector2[7];


	for (int i = 1; i <7; i++)
	{
		if (i == 6)
		{
			vector2[0] = vector1[i];
		}
		vector2[i] = vector1[i - 1];
		

		

	}
	// Insert your code here
	for (int i = 0; i < 7; i++)
		cout << vector2[i] << ' ';
	cout << endl;
	return 0;



	return 0;
}
